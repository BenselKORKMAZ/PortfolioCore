@model PortfolioCore.Entities.Experience
@{
    ViewData["Title"] = "CreateExperience";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Deneyim Ekleme Sayfası</h2>
<form method="post">
    <label>Başlık</label>
    <input type="text" class="form-control" asp-for="Title" style="border:1px solid ;" />
    <br />
    <label>Alt Başlık</label>
    <input type="text" class="form-control" asp-for="SubTitle" style="border:1px solid ;" />
    <br />
    <label>Tarih</label>
    <input type="text" class="form-control" asp-for="ExperienceDate" style="border:1px solid ;" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" asp-for="Description" style="border:1px solid;" />
    <br />
    <button class="btn btn-primary">Deneyimi Ekle</button>

</form>

