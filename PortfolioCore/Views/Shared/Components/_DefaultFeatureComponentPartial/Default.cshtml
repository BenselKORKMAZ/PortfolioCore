@model List<Feature>

<section id="hero" class="hero section light-background">
    
        <img src="/MyResume-1.0.0/assets/img/hero-bg.jpg" alt="">

        <div class="container" data-aos="zoom-out">
            <div class="row justify-content-center">
                <div class="col-lg-9">
                    <h2>Bensel KORKMAZ</h2>
                @foreach (var item in Model)
                {
                    <p>
                        Çalışma Alanlarım:
                        <span class="typed" data-typed-items="@item.JobTitle1,@item.JobTitle2, @item.JobTitle3"></span>
                        <span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span>
                    </p>
                }
                    <div class="social-links">
                    @await Component.InvokeAsync("_DefaultSocialMediaComponentPartial")
                    </div>
                
                </div>
            </div>
        </div>
    
</section>
